<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#9fbb58"/>
            <stroke
                android:width="3dip"
                android:color="#9fbb58" />
            <corners
                android:radius="13dip"/>
            <padding
                android:left="0dip"
                android:top="0dip"
                android:right="0dip"
                android:bottom="0dip" />
        </shape>
    </item>
    <item>
    <shape>
        <gradient
            android:startColor="#9fbb58"
            android:endColor="#7F9546"
            android:angle="270" />
        <stroke
            android:width="3dip"
            android:color="#9fbb58" />
        <corners
            android:radius="15dip"/>
        <padding
            android:left="0dip"
            android:top="0dip"
            android:right="0dip"
            android:bottom="0dip" />
    </shape>
</item>
</selector>